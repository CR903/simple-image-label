{
  "name": "simple-image-label",
  "private": false,
  "version": "0.0.4",
  "description": "Javascript image label, use in deep learning",
  "source": "src/simpleImageLabel.js",
  "main": "libs/simpleImageLabel.js",
  "module": "libs/simpleImageLabel.module.js",
  "scripts": {
    "start": "parcel index.html",
    "build": "parcel build --no-source-maps --no-cache --detailed-report && terser libs/simpleImageLabel.js -c passes=2 --compress --mangle --output ./libs/simpleImageLabel.js",
    "build:html": "parcel build index.html --dist-dir html --no-source-maps --no-cache --detailed-report",
    "deploy": "./deploy.sh html",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "king6180@gmail.com",
  "license": "MIT",
  "devDependencies": {
    "@parcel/transformer-less": "^2.9.3",
    "less": "^4.2.0",
    "parcel": "^2.9.3",
    "terser": "^5.19.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/king2088/simple-image-label.git"
  },
  "bugs": {
    "url": "https://github.com/king2088/simple-image-label/issues"
  },
  "homepage": "https://github.com/king2088/simple-image-label",
  "keywords": [
    "image",
    "label",
    "image label",
    "YOLO",
    "object detection",
    "deep-learning"
  ]
}
